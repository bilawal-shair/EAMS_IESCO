{
  "expo": {
    "name": "Solo-AMS",
    "slug": "EAMS-IESCO",
    "version": "1.0.0",
    "orientation": "portrait",
    "newArchEnabled": true,
    "icon": "./assets/Appicon/ic_launcher.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#fff"
     },
    "permissions": [
    "CAMERA",
    "READ_EXTERNAL_STORAGE",
    "WRITE_EXTERNAL_STORAGE"
     ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },

    "android": {
   
      "usesCleartextTraffic":true,
    
      "adaptiveIcon": {
        "foregroundImage": "./assets/Appicon/ic_launcher.png",
        "backgroundColor": "#EC1110"
      },
      "package": "com.bilawalshair.EAMSIESCO"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "1b150ed7-9ac4-473e-9bde-e55a99449e03"
      }
    },
   "plugins":[
    [
    "expo-build-properties",
    {
      "android":{
        "usesCleartextTraffic":true
      }
    }
  ]
   ]
  }
}
